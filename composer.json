{
    "name": "totten/pogo",
    "description": "Run PHP scripts with inline dependencies",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Otten",
            "email": "totten@civicrm.org"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/console": "~3.0|~4.0",
        "symfony/yaml": "~3.0|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Pogo\\": "src/"
         }
    },
    "bin": [
        "bin/pogo.php"
    ],
    "config": {
        "bin-dir": "bin"
    }
}
